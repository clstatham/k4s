%add
    push q bp
    mov q bp sp
    sub q sp $16
; %left <= rg
    mov q ra rg
; %right <= rh
    mov q rb rh
    mov q rc %add
%addstart
; %0 = add i64 %right, i64 %left
; %0 = add i64 %right, i64 %left
    mov q [-16+bp] rb
    add q [-16+bp] ra
    mov q rc %addstart
    mov q ra [-16+bp]
    add q sp $16
    pop q bp
    ret


