
%rust_begin_unwind
    push q bp
    mov q bp sp
; %_1 <= rg
    mov q ra rg
    mov q rb %rust_begin_unwind
%rust_begin_unwindstart
    mov q rb %rust_begin_unwindstart
    jmp q %rust_begin_unwindbb1
%rust_begin_unwindbb1
    mov q rb %rust_begin_unwindbb1
    jmp q %rust_begin_unwindbb1
